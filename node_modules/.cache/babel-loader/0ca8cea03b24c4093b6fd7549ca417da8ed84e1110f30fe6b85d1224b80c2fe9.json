{"ast":null,"code":"import { computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nimport houseDetails from \"@/components/HouseContent.vue\";\nexport default {\n  props: {\n    houseprop: {\n      type: null,\n      required: true\n    }\n  },\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const house = computed(() => {\n      return store.getters.getById(route.params.houseId);\n    });\n    function hasGarageToHumain(hasGarage) {\n      if (hasGarage) return \"yes\";\n      return \"no\";\n    }\n    return {\n      house,\n      hasGarageToHumain\n    };\n  },\n  components: {\n    houseDetails\n  }\n};","map":{"version":3,"mappings":"AAaA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAE,IAAI;MACVC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,EAAE;IACxB,MAAMS,KAAI,GAAIR,QAAQ,EAAE;IAExB,MAAMS,KAAI,GAAIX,QAAQ,CAAC,MAAM;MAC3B,OAAOU,KAAK,CAACE,OAAO,CAACC,OAAO,CAACJ,KAAK,CAACK,MAAM,CAACC,OAAO,CAAC;IACpD,CAAC,CAAC;IAEF,SAASC,iBAAiB,CAACC,SAAS,EAAE;MACpC,IAAIA,SAAS,EAAE,OAAO,KAAK;MAC3B,OAAO,IAAI;IACb;IACA,OAAO;MACLN,KAAK;MACLK;IACF,CAAC;EACH,CAAC;EACDE,UAAU,EAAE;IACVf;EACF;AACF,CAAC","names":["computed","useRoute","useStore","houseDetails","props","houseprop","type","required","setup","route","store","house","getters","getById","params","houseId","hasGarageToHumain","hasGarage","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HouseDetailsView.vue"],"sourcesContent":["<template>\n  <div class=\"center\">\n    <router-link to=\"/\" class=\"back-button\" style=\"text-decoration: none; color: inherit\">\n      <img src=\"@/assets/ic_back_grey@3x.png\" alt=\"ic_back\" width=\"20\" height=\"20\" />\n      <div class=\"back-button-label\">Back to overview</div>\n    </router-link>\n    <br />\n    <br />\n    <houseDetails :houseprop=\"house\"/>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nimport houseDetails from \"@/components/HouseContent.vue\"\n\nexport default {\n  props: {\n    houseprop: {\n      type: null,\n      required: true,\n    },\n  },\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n\n    const house = computed(() => {\n      return store.getters.getById(route.params.houseId);\n    });\n\n    function hasGarageToHumain(hasGarage) {\n      if (hasGarage) return \"yes\";\n      return \"no\";\n    }\n    return {\n      house,\n      hasGarageToHumain,\n    };\n  },\n  components: {\n    houseDetails,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.back-button {\n  display: flex;\n  img {\n    margin-right: 10px;\n  }\n}\n.card-details {\n  color: map-get($colors, SECONDARY);\n  padding-bottom: 50px;\n  .container {\n    margin: 40px 40px 0 40px;\n  }\n  .house-details-image {\n    width: 100%;\n  }\n  background-color: white;\n  .icon-container {\n    display: flex;\n  }\n  .desc-container {\n    display: grid;\n    text-align: left;\n  }\n  .container {\n    img {\n      margin: 0px 5px 0px 0px;\n    }\n  }\n  .spacer {\n    width: 40px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
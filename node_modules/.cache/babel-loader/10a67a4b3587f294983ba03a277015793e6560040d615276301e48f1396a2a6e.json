sets/ic_size@3x.png\" alt=\"ic_size\" width=\"20\" height=\"20\" />\n        {{ house.size }} mÂ²\n        <img src=\"@/assets/ic_construction_date@3x.png\" alt=\"ic_build\" width=\"20\" height=\"20\" />\n        Built in {{ house.constructionYear }}\n      </div>\n      <br>\n      <div class=\"icon-container listing-information text-grey\">\n        <img src=\"@/assets/ic_bed@3x.png\" alt=\"ic_bed\" width=\"20\" height=\"20\" />\n        {{ house.rooms.bedrooms }}\n        <img src=\"@/assets/ic_bath@3x.png\" alt=\"ic_bath\" width=\"20\" height=\"20\" />\n        {{ house.rooms.bathrooms }}\n        <img src=\"@/assets/ic_garage@3x.png\" alt=\"ic_garage\" width=\"20\" height=\"20\" />\n        {{ hasGarageToHumain(house.hasGarage) }} \n      </div>\n      <br /><br />\n      <div class=\"desc-container listing-information text-grey\">\n        {{ house.description }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n\n    const house = computed(() => {\n      return store.getters.getById(route.params.houseId);\n    });\n\n    function hasGarageToHumain(hasGarage) {\n      if (hasGarage) return 'yes'\n      return 'no'\n    }\n    return {\n      house,\n      hasGarageToHumain\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.card-details {\n  // margin: 20px 0 0 20px;\n  margin-bottom: 400px;\n  .container {\n    margin: 40px 40px 0 40px;\n  }\n  .house-details-image {\n    width: 100%;\n  }\n  background-color: white;\n  .icon-container {\n    display: flex;\n    background-color: white;\n    .element {\n      // margin-bottom: 5px;\n          margin: 20px 0 0 20px;\n\n    }\n  }\n  .desc-container {\n    display: grid;\n    text-align: left;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import { reactive, toRefs, onMounted, computed } from \"vue\";\n// import { ref } from 'vue';\nimport HouseContent from \"@/components/HouseContent.vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({});\n    const store = useStore();\n    const houses = computed(() => {\n      // Access state directly or use getter\n      return store.state.houses;\n    });\n    onMounted(async () => {\n      store.dispatch(\"getHouses\").then(() => {\n        // API success\n        console.log(\"data fetched\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request\");\n      });\n    });\n    return {\n      ...toRefs(state),\n      houses,\n      store\n    };\n  },\n  components: {\n    HouseContent\n  }\n};","map":{"version":3,"mappings":"AAOA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAC3D;AACA,OAAOC,YAAW,MAAO,+BAA+B;AACxD,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,CAAC,CAAC,CAAC;IAEzB,MAAMS,KAAI,GAAIJ,QAAQ,EAAC;IAEvB,MAAMK,MAAK,GAAIP,QAAQ,CAAC,MAAM;MAC5B;MACA,OAAOM,KAAK,CAACD,KAAK,CAACE,MAAM;IAC3B,CAAC,CAAC;IAEFR,SAAS,CAAC,YAAY;MACpBO,KAAI,CACDE,QAAQ,CAAC,WAAW,EACpBC,IAAI,CAAC,MAAM;QACV;QACAC,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAC,KAAK,CAAC,MAAM;QACX;QACAF,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;MACjC,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,OAAO;MACL,GAAGb,MAAM,CAACO,KAAK,CAAC;MAChBE,MAAM;MACND;IACF,CAAC;EACH,CAAC;EACDO,UAAU,EAAE;IACVZ;EACF;AACF,CAAC","names":["reactive","toRefs","onMounted","computed","HouseContent","useStore","name","setup","state","store","houses","dispatch","then","console","log","catch","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <HouseContent v-if=\"houses\" :housesprop=\"houses\" />\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs, onMounted, computed } from \"vue\";\n// import { ref } from 'vue';\nimport HouseContent from \"@/components/HouseContent.vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({})\n\n    const store = useStore()\n\n    const houses = computed(() => {\n      // Access state directly or use getter\n      return store.state.houses;\n    });\n\n    onMounted(async () => {\n      store\n        .dispatch(\"getHouses\")\n        .then(() => {\n          // API success\n          console.log(\"data fetched\");\n        })\n        .catch(() => {\n          // API fail\n          console.log(\"error in request\");\n        });\n    });\n\n    return {\n      ...toRefs(state),\n      houses,\n      store,\n    };\n  },\n  components: {\n    HouseContent,\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
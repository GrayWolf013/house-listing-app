{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, toRefs, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const store = useStore();\n    const router = useRoute();\n    const state = reactive({\n      buttonClicked: false,\n      emptyHouse: {\n        streetName: \"\",\n        houseNumber: null,\n        numberAddition: null,\n        zip: \"\",\n        city: \"\",\n        price: null,\n        size: null,\n        hasGarage: false,\n        bedrooms: null,\n        bathrooms: null,\n        constructionYear: null,\n        description: \"\"\n      }\n    });\n    const house = computed(() => {\n      return store.getters.getByIdEditModel(router.params.houseId);\n    });\n    function submitButtonClicked() {\n      state.buttonClicked = true;\n    }\n    function createHouseListing() {\n      store.dispatch(\"createHouse\", state.house).then(() => {\n        // API success\n        router.push({\n          name: \"home\"\n        });\n        console.log(\"data created\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request create\");\n      });\n    }\n    return {\n      ...toRefs(state),\n      house,\n      createHouseListing,\n      submitButtonClicked\n    };\n  }\n};","map":{"version":3,"mappings":";AAkJA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,QAAO,QAAS,KAAK;AAChD,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,EAAE;IACxB,MAAMI,MAAK,GAAIH,QAAQ,EAAE;IAEzB,MAAMI,KAAI,GAAIR,QAAQ,CAAC;MACrBS,aAAa,EAAE,KAAK;MACpBC,UAAU,EAAE;QACVC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE,IAAI;QACjBC,cAAc,EAAE,IAAI;QACpBC,GAAG,EAAE,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,IAAI;QACXC,IAAI,EAAE,IAAI;QACVC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE,IAAI;QACdC,SAAS,EAAE,IAAI;QACfC,gBAAgB,EAAE,IAAI;QACtBC,WAAW,EAAE;MACf;IACF,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAIrB,QAAQ,CAAC,MAAM;MAC3B,OAAOI,KAAK,CAACkB,OAAO,CAAC
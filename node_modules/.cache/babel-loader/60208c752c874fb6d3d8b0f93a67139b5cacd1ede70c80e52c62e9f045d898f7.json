{"ast":null,"code":"import { reactive, toRefs, onMounted, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport HouseContent from \"@/components/HouseContent.vue\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\"\n    });\n    const store = useStore();\n    function search(searchText) {\n      console.log(\"search\");\n      console.log(searchText);\n      console.log(\"search\");\n      state.searchText = searchText;\n    }\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText);\n      }\n      return store.state.houses;\n    });\n    onMounted(async () => {\n      store.dispatch(\"getHouses\").then(() => {\n        // API success\n        console.log(\"data fetched\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request\");\n      });\n    });\n    return {\n      ...toRefs(state),\n      houses,\n      search\n    };\n  },\n  components: {\n    HouseContent\n  }\n};","map":{"version":3,"mappings":"AAOA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAI;AAC1D,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,CAAC;MACrBS,UAAU,EAAE;IACd,CAAC;IAED,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IAEvB,SAASO,MAAM,CAACF,UAAU,EAAE;MAC1BG,OAAO,CAACC,GAAG,CAAC,QAAQ;MACpBD,OAAO,CAACC,GAAG,CAACJ,UAAU;MACtBG,OAAO,CAACC,GAAG,CAAC,QAAQ;MAEpBL,KAAK,CAACC,UAAS,GAAIA,UAAS;IAC9B;IAEA,MAAMK,MAAK,GAAIX,QAAQ,CAAC,MAAM;MAC5B,IAAIK,KAAK,CAACC,UAAU,EAAE;QACpB,OAAOC,KAAK,CAACK,OAAO,CAACJ,MAAM,CAACH,KAAK,CAACC,UAAU;MAC9C;MACA,OAAOC,KAAK,CAACF,KAAK,CAACM,MAAM;IAC3B,CAAC,CAAC;IAEFZ,SAAS,CAAC,YAAY;MACpBQ,KAAI,CACDM,QAAQ,CAAC,WAAW,EACpBC,IAAI,CAAC,MAAM;QACV;QACAL,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAK,KAAK,CAAC,MAAM;QACX;QACAN,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;MACjC,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,OAAO;MACL,GAAGZ,MAAM,CAACO,KAAK,CAAC;MAChBM,MAAM;MACNH;IACF,CAAC;EACH,CAAC;EACDQ,UAAU,EAAE;IACVd;EACF;AACF,CAAC","names":["reactive","toRefs","onMounted","computed","useStore","HouseContent","name","setup","state","searchText","store","search","console","log","houses","getters","dispatch","then","catch","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <HouseContent v-if=\"houses\" :housesprop=\"houses\" @search=\"search\"/>\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs, onMounted, computed } from \"vue\"\nimport { useStore } from \"vuex\"\nimport HouseContent from \"@/components/HouseContent.vue\"\n\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\"\n    })\n\n    const store = useStore()\n\n    function search(searchText) {\n      console.log(\"search\")\n      console.log(searchText)\n      console.log(\"search\")\n\n      state.searchText = searchText\n    }\n\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText) \n      }\n      return store.state.houses;\n    });\n\n    onMounted(async () => {\n      store\n        .dispatch(\"getHouses\")\n        .then(() => {\n          // API success\n          console.log(\"data fetched\");\n        })\n        .catch(() => {\n          // API fail\n          console.log(\"error in request\");\n        });\n    });\n\n    return {\n      ...toRefs(state),\n      houses,\n      search,\n    };\n  },\n  components: {\n    HouseContent,\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
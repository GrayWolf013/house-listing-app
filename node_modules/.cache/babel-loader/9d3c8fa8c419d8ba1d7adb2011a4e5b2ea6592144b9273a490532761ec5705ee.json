{"ast":null,"code":"import { computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const house = computed(() => {\n      return store.getters.getById(route.params.houseId);\n    });\n    return {\n      house\n    };\n  }\n};","map":{"version":3,"mappings":"AAsBA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,EAAE;IACxB,MAAMI,KAAI,GAAIH,QAAQ,EAAE;IAExB,MAAMI,KAAI,GAAIN,QAAQ,CAAC,MAAM;MAC3B,OAAOK,KAAK,CAACE,OAAO,CAACC,OAAO,CAACJ,KAAK,CAACK,MAAM,CAACC,OAAO,CAAC;IACpD,CAAC,CAAC;IACF,OAAO;MACLJ;IACF,CAAC;EACH;AACF,CAAC","names":["computed","useRoute","useStore","setup","route","store","house","getters","getById","params","houseId"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HouseDetailsView.vue"],"sourcesContent":["<template>\n  <div class=\"card-details center\">\n    <img :src=\"house.image\" alt=\"house imag\" />\n    <div class=\"container\">\n      <div class=\"tab-button element\">{{ house.location.street }}</div>\n      <div class=\"body-text element\">€ {{ house.price.toLocaleString(\"en-US\") }}</div>\n      <div class=\"listing-information text-grey element\">\n        {{ house.location.zip }} {{ house.location.city }}\n      </div>\n      <div class=\"listing-information text-grey\">\n        <img src=\"@/assets/ic_bed@3x.png\" alt=\"ic_bed\" width=\"10\" height=\"10\" />\n        {{ house.rooms.bedrooms }}\n        <img src=\"@/assets/ic_bath@3x.png\" alt=\"ic_bath\" width=\"10\" height=\"10\" />\n        {{ house.rooms.bathrooms }}\n        <img src=\"@/assets/ic_size@3x.png\" alt=\"ic_size\" width=\"10\" height=\"10\" />\n        {{ house.size }} m²\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n\n    const house = computed(() => {\n      return store.getters.getById(route.params.houseId);\n    });\n    return {\n      house,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.card-details {\n  // float: left;\n  img {\n    width: 100%;\n  }\n  background-color: white;\n\n  .listing-information {\n    margin: 20px 0 0 20px;\n    display: flex;\n    box-sizing: border-box;\n    flex-direction: column;\n\n    .element {\n      margin-bottom: 5px;\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
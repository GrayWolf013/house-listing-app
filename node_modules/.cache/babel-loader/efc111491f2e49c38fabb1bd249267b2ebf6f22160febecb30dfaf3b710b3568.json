{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    const state = reactive({\n      house: {\n        streetName: \"\",\n        houseNumber: 0,\n        numberAddition: 0,\n        zip: \"\",\n        city: \"\",\n        price: 0,\n        size: 0,\n        hasGarage: false,\n        bedrooms: 0,\n        bathrooms: 0,\n        constructionYear: 0,\n        description: \"\"\n      }\n    });\n    function createHouseListing() {\n      store.dispatch(\"createHouse\", state.house).then(() => {\n        // API success\n        console.log(\"data created\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request create\");\n      });\n    }\n    return {\n      ...toRefs(state),\n      createHouseListing\n    };\n  }\n};","map":{"version":3,"mappings":"AAmFA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,EAAE;IAExB,MAAMG,KAAI,GAAIL,QAAQ,CAAC;MACrBM,KAAK,EAAE;QACLC,UAAU,EAAE,EAAE;QACdC,WAAW,EAAE,CAAC;QACdC,cAAc,EAAE,CAAC;QACjBC,GAAG,EAAE,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,CAAC;QACRC,IAAI,EAAE,CAAC;QACPC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE,CAAC;QACXC,SAAS,EAAE,CAAC;QACZC,gBAAgB,EAAE,CAAC;QACnBC,WAAW,EAAE;MACf;IACF,CAAC,CAAC;IAEF,SAASC,kBAAkB,GAAG;MAC5Bf,KAAI,CACDgB,QAAQ,CAAC,aAAa,EAAEf,KAAK,CAACC,KAAK,EACnCe,IAAI,CAAC,MAAM;QACV;QACAC,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAC,KAAK,CAAC,MAAM;QACX;QACAF,OAAO,CAACC,GAAG,CAAC,yBAAyB,CAAC;MACxC,CAAC,CAAC;IACN;IAEA,OAAO;MACL,GAAGtB,MAAM,CAACI,KAAK,CAAC;MAChBc;IACF,CAAC;EACH;AACF,CAAC","names":["reactive","toRefs","useStore","setup","store","state","house","streetName","houseNumber","numberAddition","zip","city","price","size","hasGarage","bedrooms","bathrooms","constructionYear","description","createHouseListing","dispatch","then","console","log","catch"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/CreateHouseView.vue"],"sourcesContent":["<template>\n  <div class=\"root\">\n    <div class=\"create-house-container\">\n      <router-link to=\"/\" style=\"text-decoration: none; color: inherit\">\n        <div class=\"back-button\">\n          <img src=\"@/assets/ic_back_grey@3x.png\" alt=\"ic_back\" width=\"20\" height=\"20\" />\n          <div class=\"back-button-label\">Back to overview</div>\n        </div>\n      </router-link>\n      <div>\n        <div class=\"header1\">Create new listing</div>\n        <div class=\"input-field-title\">Street name*</div>\n        <input\n          type=\"text\"\n          placeholder=\"Enter the street name\"\n          v-model=\"house.streetName\"\n        />\n        <div class=\"flex\">\n          <div>\n            <div class=\"input-field-title\">House number*</div>\n            <input\n              type=\"text\"\n              placeholder=\"Enter the house number\"\n              v-model=\"house.houseNumber\"\n            />\n          </div>\n          <div class=\"spacer\" />\n          <div>\n            <div class=\"input-field-title\">Addition (optional)</div>\n            <input type=\"text\" placeholder=\"e.g. A\" v-model=\"house.numberAddition\" />\n          </div>\n        </div>\n        <div class=\"input-field-title\">Postal Code*</div>\n        <input type=\"text\" placeholder=\"e.g. 1000 AA\" v-model=\"house.zio\" />\n        <div class=\"input-field-title\">City*</div>\n        <input type=\"text\" placeholder=\"e.g. Utrecht\" v-model=\"house.city\" />\n        <div class=\"input-field-title\">Upload picture (PNG or JPG)*</div>\n        <img src=\"@/assets/ic_upload@3x.png\" alt=\"upload\" />\n        <div class=\"input-field-title\">Price*</div>\n        <input type=\"text\" placeholder=\"e.g. €150.000\" v-model=\"house.price\" />\n        <div class=\"flex\">\n          <div>\n            <div class=\"input-field-title\">Size*</div>\n            <input type=\"text\" placeholder=\"e.g. 60m²\" v-model=\"house.size\" />\n          </div>\n          <div class=\"spacer\" />\n          <div>\n            <div class=\"input-field-title\">Garage*</div>\n            <input type=\"text\" placeholder=\"Select\" v-model=\"house.hasGarage\" />\n          </div>\n        </div>\n        <div class=\"flex\">\n          <div>\n            <div class=\"input-field-title\">Bedrooms*</div>\n            <input type=\"text\" placeholder=\"Enter amount\" v-model=\"house.bedrooms\" />\n          </div>\n          <div class=\"spacer\" />\n          <div>\n            <div class=\"input-field-title\">Bathrooms*</div>\n            <input type=\"text\" placeholder=\"Enter amount\" v-model=\"house.bathrooms\" />\n          </div>\n        </div>\n        <div class=\"input-field-title\">Construction date*</div>\n        <input type=\"text\" placeholder=\"e.g. 1900\" v-model=\"house.constructionYear\" />\n        <div class=\"input-field-title\">Description*</div>\n        <textarea\n          name=\"\"\n          id=\"\"\n          cols=\"30\"\n          rows=\"10\"\n          placeholder=\"Enter description\"\n          v-model=\"house.description\"\n        />\n        <div class=\"btn-container\">\n          <button class=\"btn\" @click=\"createHouseListing\">POST</button>\n        </div>\n        <br />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs } from \"vue\";\nimport { useStore } from \"vuex\";\n\nexport default {\n  setup() {\n    const store = useStore();\n\n    const state = reactive({\n      house: {\n        streetName: \"\",\n        houseNumber: 0,\n        numberAddition: 0,\n        zip: \"\",\n        city: \"\",\n        price: 0,\n        size: 0,\n        hasGarage: false,\n        bedrooms: 0,\n        bathrooms: 0,\n        constructionYear: 0,\n        description: \"\",\n      },\n    });\n\n    function createHouseListing() {\n      store\n        .dispatch(\"createHouse\", state.house)\n        .then(() => {\n          // API success\n          console.log(\"data created\");\n        })\n        .catch(() => {\n          // API fail\n          console.log(\"error in request create\");\n        });\n    }\n\n    return {\n      ...toRefs(state),\n      createHouseListing,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.root {\n  margin: 0;\n  padding: 0;\n  background-image: url(\"/src/assets/img_background@3x.png\");\n  background-repeat: no-repeat;\n  background-size: 105%;\n\n  .create-house-container {\n    margin: 30px 0 0 15%;\n    .back-button {\n      margin-bottom: 20px;\n    }\n    .header1 {\n      margin-bottom: 20px;\n    }\n\n    .flex {\n      display: flex;\n    }\n    .input-field-title {\n      margin: 10px 0;\n    }\n    .spacer {\n      width: 20px;\n    }\n    .btn-container {\n      margin: 50px 275px;\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import { reactive, toRefs, onMounted, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport HouseContent from \"@/components/HouseContent.vue\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\",\n      sortByPrice: true\n    });\n    const store = useStore();\n    function search(searchText) {\n      state.searchText = searchText;\n    }\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText);\n      }\n      return store.state.houses;\n    });\n    onMounted(async () => {\n      store.dispatch(\"getHouses\").then(() => {\n        // API success\n        console.log(\"data fetched\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request\");\n      });\n    });\n    return {\n      ...toRefs(state),\n      houses,\n      search\n    };\n  },\n  components: {\n    HouseContent\n  }\n};","map":{"version":3,"mappings":"AAOA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAI;AAC1D,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,CAAC;MACrBS,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE;IACf,CAAC;IAED,MAAMC,KAAI,GAAIP,QAAQ,EAAC;IAEvB,SAASQ,MAAM,CAACH,UAAU,EAAE;MAC1BD,KAAK,CAACC,UAAS,GAAIA,UAAS;IAC9B;IAEA,MAAMI,MAAK,GAAIV,QAAQ,CAAC,MAAM;MAC5B,IAAIK,KAAK,CAACC,UAAU,EAAE;QACpB,OAAOE,KAAK,CAACG,OAAO,CAACF,MAAM,CAACJ,KAAK,CAACC,UAAU;MAC9C;MACA,OAAOE,KAAK,CAACH,KAAK,CAACK,MAAM;IAC3B,CAAC,CAAC;IAEFX,SAAS,CAAC,YAAY;MACpBS,KAAI,CACDI,QAAQ,CAAC,WAAW,EACpBC,IAAI,CAAC,MAAM;QACV;QACAC,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAC,KAAK,CAAC,MAAM;QACX;QACAF,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;MACjC,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,OAAO;MACL,GAAGjB,MAAM,CAACO,KAAK,CAAC;MAChBK,MAAM;MACND;IACF,CAAC;EACH,CAAC;EACDQ,UAAU,EAAE;IACVf;EACF;AACF,CAAC","names":["reactive","toRefs","onMounted","computed","useStore","HouseContent","name","setup","state","searchText","sortByPrice","store","search","houses","getters","dispatch","then","console","log","catch","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <HouseContent v-if=\"houses\" :housesprop=\"houses\" @search=\"search\" @sortBy=\"sortBy\"/>\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs, onMounted, computed } from \"vue\"\nimport { useStore } from \"vuex\"\nimport HouseContent from \"@/components/HouseContent.vue\"\n\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\",\n      sortByPrice: true\n    })\n\n    const store = useStore()\n\n    function search(searchText) {\n      state.searchText = searchText\n    }\n\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText) \n      }\n      return store.state.houses;\n    });\n\n    onMounted(async () => {\n      store\n        .dispatch(\"getHouses\")\n        .then(() => {\n          // API success\n          console.log(\"data fetched\");\n        })\n        .catch(() => {\n          // API fail\n          console.log(\"error in request\");\n        });\n    });\n\n    return {\n      ...toRefs(state),\n      houses,\n      search,\n    };\n  },\n  components: {\n    HouseContent,\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
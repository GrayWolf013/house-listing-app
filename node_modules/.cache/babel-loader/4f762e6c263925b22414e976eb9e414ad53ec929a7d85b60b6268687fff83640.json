{"ast":null,"code":"import { reactive, toRefs, onMounted, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport HouseContent from \"@/components/HouseContent.vue\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\",\n      sortByPrice: true\n    });\n    const store = useStore();\n    function search(searchText) {\n      state.searchText = searchText;\n    }\n    function sortBy(price) {\n      console.log(\"sortByPrice Home\");\n      state.sortByPrice = price;\n    }\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText);\n      }\n      return store.getters.sortBy(state.sortByPrice);\n    });\n    onMounted(async () => {\n      store.dispatch(\"getHouses\").then(() => {\n        // API success\n        console.log(\"data fetched\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request\");\n      });\n    });\n    return {\n      ...toRefs(state),\n      houses,\n      search,\n      sortBy\n    };\n  },\n  components: {\n    HouseContent\n  }\n};","map":{"version":3,"mappings":"AAOA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAI;AAC1D,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,YAAW,MAAO,+BAA8B;AAEvD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,CAAC;MACrBS,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE;IACf,CAAC;IAED,MAAMC,KAAI,GAAIP,QAAQ,EAAC;IAEvB,SAASQ,MAAM,CAACH,UAAU,EAAE;MAC1BD,KAAK,CAACC,UAAS,GAAIA,UAAS;IAC9B;IAEA,SAASI,MAAM,CAACC,KAAK,EAAE;MACrBC,OAAO,CAACC,GAAG,CAAC,kBAAkB;MAC9BR,KAAK,CAACE,WAAU,GAAII,KAAI;IAC1B;IAEA,MAAMG,MAAK,GAAId,QAAQ,CAAC,MAAM;MAC5B,IAAIK,KAAK,CAACC,UAAU,EAAE;QACpB,OAAOE,KAAK,CAACO,OAAO,CAACN,MAAM,CAACJ,KAAK,CAACC,UAAU;MAC9C;MACA,OAAOE,KAAK,CAACO,OAAO,CAACL,MAAM,CAACL,KAAK,CAACE,WAAW;IAC/C,CAAC,CAAC;IAEFR,SAAS,CAAC,YAAY;MACpBS,KAAI,CACDQ,QAAQ,CAAC,WAAW,EACpBC,IAAI,CAAC,MAAM;QACV;QACAL,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAK,KAAK,CAAC,MAAM;QACX;QACAN,O
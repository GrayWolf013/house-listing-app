{"ast":null,"code":"import { reactive, toRefs } from \"vue\";\nimport DeleteConfirmationAlert from \"./Alerts/DeleteConfirmationAlert.vue\";\nexport default {\n  name: \"HouseDetailsCard\",\n  props: {\n    house: {\n      type: null,\n      required: true\n    }\n  },\n  setup(props, context) {\n    const state = reactive({\n      showAlert: false\n    });\n    function hasGarageToHumain(hasGarage) {\n      if (hasGarage) return \"yes\";\n      return \"no\";\n    }\n    function toggleAlert(showAlert) {\n      state.showAlert = showAlert;\n      context.emit(\"toggleAlert\", showAlert);\n    }\n    return {\n      ...toRefs(state),\n      hasGarageToHumain,\n      toggleAlert\n    };\n  },\n  emits: [\"toggleAlert\"],\n  components: {\n    DeleteConfirmationAlert\n  }\n};","map":{"version":3,"mappings":"AA6GA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,OAAOC,uBAAsB,MAAO,sCAAsC;AAE1E,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAE,IAAI;MACVC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAK,CAACJ,KAAK,EAAEK,OAAO,EAAE;IACpB,MAAMC,KAAI,GAAIV,QAAQ,CAAC;MACrBW,SAAS,EAAE;IACb,CAAC,CAAC;IAEF,SAASC,iBAAiB,CAACC,SAAS,EAAE;MACpC,IAAIA,SAAS,EAAE,OAAO,KAAK;MAC3B,OAAO,IAAI;IACb;IAEA,SAASC,WAAW,CAACH,SAAS,EAAE;MAC9BD,KAAK,CAACC,SAAQ,GAAIA,SAAS;MAC3BF,OAAO,CAACM,IAAI,CAAC,aAAa,EAAEJ,SAAS,CAAC;IACxC;IAEA,OAAO;MACL,GAAGV,MAAM,CAACS,KAAK,CAAC;MAChBE,iBAAiB;MACjBE;IACF,CAAC;EACH,CAAC;EACDE,KAAK,EAAE,CAAC,aAAa,CAAC;EACtBC,UAAU,EAAE;IACVf;EACF;AACF,CAAC","names":["reactive","toRefs","DeleteConfirmationAlert","name","props","house","type","required","setup","context","state","showAlert","hasGarageToHumain","hasGarage","toggleAlert","emit","emits","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/components/HouseDetailsCardComponent.vue"],"sourcesContent":["<template>\n  <div class=\"card-details\">\n    <img\n      class=\"house-details-image\"\n      v-if=\"house.image\"\n      :src=\"house.image\"\n      alt=\"HouseImg\"\n    />\n    <img\n      class=\"house-details-image\"\n      v-else\n      src=\"@/assets/img_placeholder_house@3x.png\"\n      alt=\"HouseImg\"\n    />\n    <div class=\"container\">\n      <div class=\"card-header\">\n        <div class=\"tab-button element\">{{ house.location.street }}</div>\n        <div class=\"card-header-manage\">\n          <router-link\n            v-if=\"house.id\"\n            style=\"text-decoration: none; color: inherit\"\n            :to=\"{ name: 'updateHouse', params: { houseId: house.id } }\"\n          >\n            <img\n              v-if=\"house.madeByMe\"\n              src=\"@/assets/ic_edit@3x.png\"\n              alt=\"delete-icon\"\n              width=\"15\"\n              height=\"15\"\n            />\n          </router-link>\n\n          <img\n            v-if=\"house.madeByMe\"\n            src=\"@/assets/ic_delete@3x.png\"\n            alt=\"delete-icon\"\n            width=\"15\"\n            height=\"15\"\n            @click.prevent=\"toggleAlert(true)\"\n          />\n        </div>\n      </div>\n      <br />\n      <div class=\"icon-container listing-information\">\n        <img\n          src=\"@/assets/ic_location@3x.png\"\n          alt=\"ic_bed\"\n          width=\"20\"\n          height=\"20\"\n        />\n        {{ house.location.zip }} {{ house.location.city }}\n      </div>\n      <br />\n      <div class=\"icon-container listing-information\">\n        <img\n          src=\"@/assets/ic_price@3x.png\"\n          alt=\"ic_price\"\n          width=\"20\"\n          height=\"20\"\n        />\n        {{ house.price.toLocaleString(\"en-US\") }}\n        <div class=\"spacer\"></div>\n        <img\n          src=\"@/assets/ic_size@3x.png\"\n          alt=\"ic_size\"\n          width=\"20\"\n          height=\"20\"\n        />\n        {{ house.size }} mÂ²\n        <div class=\"spacer\"></div>\n        <img\n          src=\"@/assets/ic_construction_date@3x.png\"\n          alt=\"ic_build\"\n          width=\"20\"\n          height=\"20\"\n        />\n        Built in {{ house.constructionYear }}\n      </div>\n      <br />\n      <div class=\"icon-container listing-information\">\n        <img src=\"@/assets/ic_bed@3x.png\" alt=\"ic_bed\" width=\"20\" height=\"20\" />\n        {{ house.rooms.bedrooms }}\n        <div class=\"spacer\"></div>\n        <img\n          src=\"@/assets/ic_bath@3x.png\"\n          alt=\"ic_bath\"\n          width=\"20\"\n          height=\"20\"\n        />\n        {{ house.rooms.bathrooms }}\n        <div class=\"spacer\"></div>\n        <img\n          src=\"@/assets/ic_garage@3x.png\"\n          alt=\"ic_garage\"\n          width=\"20\"\n          height=\"20\"\n        />\n        {{ hasGarageToHumain(house.hasGarage) }}\n      </div>\n      <br /><br />\n      <div class=\"desc-container listing-information\">\n        {{ house.description }}\n      </div>\n    </div>\n  </div>\n  <DeleteConfirmationAlert :houseId=\"houseprop.id\" v-if=\"showAlert\" @hideAlert=\"toggleAlert(false)\" />\n</template>\n\n<script>\nimport { reactive, toRefs } from \"vue\";\nimport DeleteConfirmationAlert from \"./Alerts/DeleteConfirmationAlert.vue\";\n\nexport default {\n  name: \"HouseDetailsCard\",\n  props: {\n    house: {\n      type: null,\n      required: true,\n    },\n  },\n  setup(props, context) {\n    const state = reactive({\n      showAlert: false,\n    });\n\n    function hasGarageToHumain(hasGarage) {\n      if (hasGarage) return \"yes\";\n      return \"no\";\n    }\n\n    function toggleAlert(showAlert) {\n      state.showAlert = showAlert;\n      context.emit(\"toggleAlert\", showAlert);\n    }\n\n    return {\n      ...toRefs(state),\n      hasGarageToHumain,\n      toggleAlert\n    };\n  },\n  emits: [\"toggleAlert\"],\n  components: {\n    DeleteConfirmationAlert,\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.card-details {\n  color: map-get($colors, SECONDARY);\n  padding-bottom: 50px;\n  background-color: white;\n  .container {\n    margin: 40px 40px 0 40px;\n  }\n  .house-details-image {\n    width: 100%;\n  }\n  .card-header {\n    display: flex;\n    justify-content: space-between;\n    .card-header-manage {\n      display: flex;\n      justify-content: left;\n      * {\n        margin-left: 10px;\n      }\n    }\n  }\n  .icon-container {\n    display: flex;\n  }\n  .desc-container {\n    display: grid;\n    text-align: left;\n  }\n  .container {\n    img {\n      margin: 0px 5px 0px 0px;\n    }\n  }\n  .spacer {\n    width: 40px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
{"ast":null,"code":"import { reactive, toRefs, onMounted, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport HouseContent from \"@/components/HouseContentComponent.vue\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\",\n      sortByPrice: true\n    });\n    const store = useStore();\n    function search(searchText) {\n      state.searchText = searchText;\n    }\n    function sortBy(price) {\n      state.sortByPrice = price;\n    }\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText);\n      }\n      return store.getters.sortBy(state.sortByPrice);\n    });\n    onMounted(async () => {\n      store.dispatch(\"getHouses\").then(() => {\n        // API success\n        console.log(\"data fetched\");\n      }).catch(() => {\n        // API fail\n        console.log(\"error in request\");\n      });\n    });\n    return {\n      ...toRefs(state),\n      houses,\n      search,\n      sortBy\n    };\n  },\n  components: {\n    HouseContent\n  }\n};","map":{"version":3,"mappings":"AAcA,SAASA,QAAQ,EAAEC,MAAM,EAAEC,SAAS,EAAEC,QAAO,QAAS,KAAK;AAC3D,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,YAAW,MAAO,wCAAwC;AAEjE,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIR,QAAQ,CAAC;MACrBS,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAIP,QAAQ,EAAE;IAExB,SAASQ,MAAM,CAACH,UAAU,EAAE;MAC1BD,KAAK,CAACC,UAAS,GAAIA,UAAU;IAC/B;IAEA,SAASI,MAAM,CAACC,KAAK,EAAE;MACrBN,KAAK,CAACE,WAAU,GAAII,KAAK;IAC3B;IAEA,MAAMC,MAAK,GAAIZ,QAAQ,CAAC,MAAM;MAC5B,IAAIK,KAAK,CAACC,UAAU,EAAE;QACpB,OAAOE,KAAK,CAACK,OAAO,CAACJ,MAAM,CAACJ,KAAK,CAACC,UAAU,CAAC;MAC/C;MACA,OAAOE,KAAK,CAACK,OAAO,CAACH,MAAM,CAACL,KAAK,CAACE,WAAW,CAAC;IAChD,CAAC,CAAC;IAEFR,SAAS,CAAC,YAAY;MACpBS,KAAI,CACDM,QAAQ,CAAC,WAAW,EACpBC,IAAI,CAAC,MAAM;QACV;QACAC,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,EACAC,KAAK,CAAC,MAAM;QACX;QACAF,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;MACjC,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,OAAO;MACL,GAAGnB,MAAM,CAACO,KAAK,CAAC;MAChBO,MAAM;MACNH,MAAM;MACNC;IACF,CAAC;EACH,CAAC;EACDS,UAAU,EAAE;IACVjB;EACF;AACF,CAAC","names":["reactive","toRefs","onMounted","computed","useStore","HouseContent","name","setup","state","searchText","sortByPrice","store","search","sortBy","price","houses","getters","dispatch","then","console","log","catch","components"],"sourceRoot":"","sources":["/Users/frostmoon/Documents/DTT projects/Vue js/house-listing-app/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <HouseContent\n      v-if=\"houses\"\n      :housesprop=\"houses\"\n      @search=\"search\"\n      @sortBy=\"sortBy\"\n    />\n  </div>\n  <router-view />\n\n</template>\n\n<script>\nimport { reactive, toRefs, onMounted, computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport HouseContent from \"@/components/HouseContentComponent.vue\";\n\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const state = reactive({\n      searchText: \"\",\n      sortByPrice: true,\n    });\n\n    const store = useStore();\n\n    function search(searchText) {\n      state.searchText = searchText;\n    }\n\n    function sortBy(price) {\n      state.sortByPrice = price;\n    }\n\n    const houses = computed(() => {\n      if (state.searchText) {\n        return store.getters.search(state.searchText);\n      }\n      return store.getters.sortBy(state.sortByPrice);\n    });\n\n    onMounted(async () => {\n      store\n        .dispatch(\"getHouses\")\n        .then(() => {\n          // API success\n          console.log(\"data fetched\");\n        })\n        .catch(() => {\n          // API fail\n          console.log(\"error in request\");\n        });\n    });\n\n    return {\n      ...toRefs(state),\n      houses,\n      search,\n      sortBy,\n    };\n  },\n  components: {\n    HouseContent,\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}